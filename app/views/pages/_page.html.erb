<div id="<%= dom_id page %>">
  <p>
    <strong>User:</strong>
    <%= page.user_id %>
  </p>

  <p>
    <strong>Title:</strong>
    <%= page.title %>
  </p>

  <p>
    <strong>Slug:</strong>
    <%= page.slug %>
  </p>

  <p>
    <strong>Subtitle:</strong>
    <%= page.subtitle %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= page.body %>
  </p>

  <p>
    <strong>Published at:</strong>
    <%= page.published_at %>
  </p>

  <p>
    <strong>Archived at:</strong>
    <%= page.archived_at %>
  </p>

</div>
